{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ZoomEndpoints": {
    "EndPoints": [
      {
        "key": "users",
        "url": "https://api.zoom.us/v2/users"
      },
      {
        "key": "usersToken",
        "url": "https://api.zoom.us/v2/users/userId/token"
      },
      {
        "key": "currentUser",
        "url": "https://api.zoom.us/v2/users/me"
      },
      {
        "key": "createMeeting",
        "url": "https://api.zoom.us/v2/users/userId/meetings"
      }
    ]
  },
  "AllowedHosts": "*"
}
